    </script>
    <script type="text/javascript">
        window.plus = "<%= image_url('plus.png') %>";
    </script>
    <script type="text/javascript">
        window.xbut = "<%= image_url('x.png') %>";
    </script>
    <script type="text/javascript">
        window.splash2 = "<%= image_url('splash2.jpg') %>";
    </script>

     
  </head>

  <body>
    <%= yield %>
  </body>
</html>
