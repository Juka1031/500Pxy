<!DOCTYPE html>
<html>
  <head>
    <title>Pxy</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
     <%= javascript_include_tag 'application' %>
     <script type="text/javascript">
        window.vine = "<%= image_url('2.PNG') %>";
    </script>
     <script type="text/javascript">
        window.pencil = "<%= image_url('3.PNG') %>";
    </script>
     <script type="text/javascript">
        window.graph = "<%= image_url('4.PNG') %>";
    </script>
     <script type="text/javascript">
        window.logo = "<%= image_url('Logo.PNG') %>";
    </script>
    <script type="text/javascript">
        window.avatar = "<%= image_url('avatar.PNG') %>";
    </script>
    <script type="text/javascript">
        window.uparrow = "<%= image_url('uparrow.PNG') %>";
    </script>
    <script type="text/javascript">
        window.max = "<%= image_url('max.png') %>";
    </script>
    <script type="text/javascript">
        window.min = "<%= image_url('min.png') %>";
    </script>

    <script type="text/javascript">
        window.default_thumb = "<%= image_url('default_thumb.PNG') %>";
    </script>
     <script type="text/javascript">
        window.up_arrow_2 = "<%= image_url('uparrow2.PNG') %>";
